extends layout

block content
  h1= title
  - var image_names = ["broccoli.jpg", "carrot.jpg", "cauliflower.jpg", "cucumbers.jpg", "Potato.jpg"];
    - for(let i = 0; i < 3; i++) {
        - let ranValue = Math.floor(Math.random() * 5);
        img(src=`/images/${image_names[ranValue]}`, alt="Vegetables Image", height="300px", width="300px")
    - }